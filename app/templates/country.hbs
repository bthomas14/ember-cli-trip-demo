<div class="container">
  <div class='row'>
    <div class='col-md-3'>
      <div class='h4 strong row-space-1 col-sm-12'>Regions</div>
      <div class="list-group">
        {{#each region in model}}
          {{#link-to 'country.region' region.id classNames='list-group-item'}}
            {{region.name}}
          {{/link-to}}
        {{/each}}
      </div>
    </div>
    <div class="col-md-9">
      {{outlet}}
    </div>
  </div>
</div>
